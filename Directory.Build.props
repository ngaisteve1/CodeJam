		<NoWarn Condition=" '(TargetFramework)' != 'netcoreapp1.0' and '(TargetFramework)' != 'netcoreapp1.1' ">$(NoWarn);NETSDK1138</NoWarn>
	</PropertyGroup>
</Project>
